<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Code Express</title>
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet">
     <link href="@Url.Content("~/Content/css/estilo2.css")" rel="stylesheet">
    <script src="@Url.Content("http://code.jquery.com/jquery.js")"></script>
    <script src="@Url.Content("~/Content/js/jsbootstrap/jquery.js")"></script>
    <script src="@Url.Content("~/Content/js/jsbootstrap/bootstrap.min.js")"></script>
    <link href="@Url.Content("~/Content/css/select2.css")"  rel="stylesheet">
    <script src="@Url.Content("~/Content/js/jsSelect2/select2.js")"></script>
    <script src="@Url.Content("~/Content/js/jsSelect2/combo.js")"></script>
 
</head>
<body>
    <!-- Tab bootstrap-->
              <nav class="navbar navbar-default" role="navigation">
              <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                 
                  <a class="navbar-brand" href="#">Menu</a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                  <ul class="nav navbar-nav">

                       <!-- Combo para Album-->
                      
                      <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Album<span class="caret"></span></a>
                      <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Crear</a></li>
                        <li><a href="#">Editar</a></li>
                        <li><a href="#">Listar</a></li>
                      </ul>
                    </li>
                  </ul>
                     <!-- Combo para Cancion-->
                    <ul class="nav navbar-nav">
                    <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Canci&oacute;n<span class="caret"></span></a>
                      <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Crear</a></li>
                        <li><a href="#">Editar</a></li>
                        <li><a href="#">Listar</a></li>
                      </ul>
                    </li>
                  </ul>
                     <!-- Combo para Artista-->
                     <ul class="nav navbar-nav">
                      <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Artista<span class="caret"></span></a>
                      <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Crear</a></li>
                        <li><a href="#">Editar</a></li>
                        <li><a href="#">Listar</a></li>
                      </ul>
                    </li>
                  </ul>
                  
                 <!-- Boton para salir de LogIn -->
                 <button type="submit" class="btn btn-default submit">LogOut</button>
                
                
                </div><!-- /.navbar-collapse -->
              </div><!-- /.container-fluid -->
            </nav>
      <!-- Tab bootstrap-->

    <div id="Contenido">
      
        <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
    </div>
    <footer>
            <p>Este es el pie va informacion</p>
        </footer>
</body>
</html>